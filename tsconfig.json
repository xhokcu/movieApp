{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@app/*": ["app/*"],
      "@assets/*": ["assets/*"],
      "@config/*": ["config/*"],
      "@src/*": ["src/*"],
      "@common/*": ["src/common/*"],
      "@components/*": ["/src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@services/*": ["src/services"],
      "@types/*": ["src/types"]
    }
  }
}
